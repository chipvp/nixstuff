alias sobash='source ~/.bash_profile'
alias soz='source ~/.zshrc'
alias ll='ls -laGh'
alias lt='ls -latrGh'
alias rm='rm -i'
alias vim='/Applications/MacVim.app/Contents/MacOS/Vim'
alias vi='vim'
alias bashrc='vim ~/.bash_profile'
alias vimrc='vim ~/.vimrc'
alias gitgraph='git log --graph --pretty=format:"%x09%C(auto)%h%C(reset)%x09%C(bold white)%s%C(reset) %C(bold dim white)- %an%C(reset) %x09%C(blue)(%ar)%C(reset)" --abbrev-commit'
alias gitignored='git ls-files --other --ignored --exclude-standard'
alias gitbranch='git remote show origin'
alias gitstat='git status'
alias effit='git commit -a -m "$(curl http://whatthecommit.com/index.txt)"'
alias ifex='dig +short myip.opendns.com @resolver1.opendns.com'
alias brewup='brew update && brew upgrade && brew cask upgrade'
